{
    "name": "ilzrv/laravel-slow-query-detector",
    "description": "Laravel Slow DB Query Detector",
    "require": {
        "illuminate/database": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/log": "^5.6|^6.0|^7.0|^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ilia Lazarev",
            "email": "me@ilzrv.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ilzrv\\LaravelSlowQueryDetector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ilzrv\\LaravelSlowQueryDetector\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ilzrv\\LaravelSlowQueryDetector\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
        "orchestra/testbench": "^3.0 || ^4.0 || ^5.0 || ^6.0",
        "timacdonald/log-fake": "^1.6"
    }
}
